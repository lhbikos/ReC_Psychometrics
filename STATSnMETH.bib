	keywords = {Racism, Blacks, Racial and Ethnic Groups, Test Construction, activism, Activism, anti-Black racism, Black Lives Matter, critical consciousness, Critical Thinking, Discrimination, Feminism, Intervention, Prevention, racial trauma, Trauma},
	pages = {1--16},
