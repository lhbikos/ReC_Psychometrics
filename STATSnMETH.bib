	keywords = {Racism, Asians, Microaggression, Factor Analysis, measure development, Test Construction, Test Validity, Human Females, Human Sex Differences, intersectionality, Sex Role Attitudes, gendered racism, gendered racial microaggressions, Asian American women},
	pages = {571--585},
