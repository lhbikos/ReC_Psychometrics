	pmcid = {PMC1495268},
	keywords = {Depression, Humans, Female, Male, Surveys and Questionnaires, Middle Aged, Reproducibility of Results, Adult, Psychological Tests, Severity of Illness Index},
	pages = {606--613},
	file = {Full Text:C\:\\Users\\lhbik\\Zotero\\storage\\DWLJIR6F\\Kroenke et al. - 2001 - The PHQ-9 validity of a brief depression severity.pdf:application/pdf},
