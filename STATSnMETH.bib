	year = {2018},
	note = {Publisher: American Psychological Association},
	keywords = {Experimentation, Psychology, Sciences, Humans, Research Design, Statistical Data, Data Sharing, big data, Big Data, data science, instrumentation, machine learning, Machine Learning},
