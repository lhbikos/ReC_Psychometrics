	keywords = {Depression, Society, Humans, Latinos/Latinas, Stress, Test Construction, Colonialism, Acculturation, Structural Equation Modeling, Major Depression, depression, Hispanic Americans, acculturative stress, colonial mentality, mainland Puerto Ricans, Puerto Rico},
	pages = {396--408},
