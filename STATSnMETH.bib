	note = {Publisher: Wiley-Blackwell Publishing Ltd.},
	keywords = {Oppression, Social Justice, Minority Groups, agency, ancestral pride, collective memory, Collective Memory, collective orientation, cultural relevance, faith, Faith, Future, future orientation, History, hope, Hope, individual orientation, Individualism, meaning, oppression, past orientation, people of color, Pride, Psychological Theories, psychological well-being, purpose, race, Race and Ethnic Discrimination, Racial and Ethnic Relations, radical hope, Radical Movements, resistance, Well Being},
	file = {Full Text:C\:\\Users\\lhbik\\Zotero\\storage\\M2H3NLHF\\Mosley et al. - 2020 - Radical hope in revolting times Proposing a cultu.pdf:application/pdf},
