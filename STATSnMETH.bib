	note = {Publisher: John Wiley \& Sons},
	keywords = {Economics, Intention, Organizational Behavior, Occupational Stress, Depression (Emotion), economic stress, economics, entrepreneurship, Entrepreneurship, Financial Strain, organizational behavior, social ties, Withdrawal (Defense Mechanism), withdrawal intentions},
	pages = {789--810},
	file = {EBSCO Full Text:C\:\\Users\\lhbik\\Zotero\\storage\\BUNI2W4X\\Pollack et al. - 2012 - The moderating role of social ties on entrepreneur.pdf:application/pdf},
