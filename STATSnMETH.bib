@article{mckinley_you_2022,
	title = {“{You} {Can} {Get} {Away} with {Anything} {Here}… {No} {Justice} at {All}”— {Sexual} {Violence} {Against} {U}.{S}. {Indigenous} {Females} and {Its} {Consequences}},
	volume = {39},
