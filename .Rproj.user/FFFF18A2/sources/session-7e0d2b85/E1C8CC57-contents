```{r}
ExpStgw1 <- dplyr::select(w1, ExprStig_1, ExprStig_2r, ExprStig_3, ExprStig_4)
ExpStgw2 <- dplyr::select(w2, ExprStig_1, ExprStig_2r, ExprStig_3, ExprStig_4 )
ExpStgw3 <- dplyr::select(w3, ExprStig_1, ExprStig_2r, ExprStig_3, ExprStig_4)
ExpStgw4 <- dplyr::select(w4, ExprStig_1, ExprStig_2r, ExprStig_3, ExprStig_4)
